{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "spring": {
    "application": {
      "name": "p16Transaction" // mention your app name . Dont use _ in name.
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8161/eureka/", //Eureka service
      "shouldRegisterWithEureka": true,
      "shouldFetchRegistry": false
    },
    "instance": {
      "port": 9164 // mention your port no of http url
    }
  }
  ,
  "AllowedHosts": "*",
  "AllowedOrigins": [ "http://localhost:3016" ]
}
